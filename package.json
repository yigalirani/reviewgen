{
  "name": "reviewgen",
  "version": "1.0.0",
  "description": "Web application that generates content using cloud API with prompt from file",
  "main": "dist/server.js",
  "scripts": {
    "build": "esbuild src/server.ts --bundle --platform=node --target=node18 --format=cjs --outfile=dist/server.js --sourcemap",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts"
  },
  "keywords": [
    "node",
    "typescript",
    "express",
    "claude",
    "anthropic"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "esbuild": "^0.27.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
